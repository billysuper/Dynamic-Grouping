@page "/"
@using Dynamic_Grouping.Data
@using Dynamic_Grouping.Data.Service
@inject GetCfgService getCfg
@inject GetHostsService getHosts
@inject SharedDataService shareData
@inject VlanOperation VO


<PageTitle>Index</PageTitle>
<head>Login with different roles.</head>
<SurveyPrompt Title="How is Blazor working for you?" />

@code {
    protected override async Task OnInitializedAsync()
    {
        getCfg.GetJson();
        getHosts.getHosts();
        shareData.vlanIfaces = VO.getVlanCfg(getHosts.hostsData,getCfg.JsonData,shareData.vlanIfaces);
    }
}